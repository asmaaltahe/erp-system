                </search>
            </field>
        </record>

    <record model="ir.actions.act_window" id="education_program_list_action">
        <field name="name">Program</field>
        <field name="res_model">program.program</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
